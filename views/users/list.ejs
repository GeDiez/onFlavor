<html>
  <head>
    <%- include("./partials/head",{title:"Users"}) %>
  </head>
  <body>
    <h2>Users</h2>
    <ul>
      <% users.forEach(function(user) { %>
        <li>
          <%= user.name %>
          <a href="http://localhost:3000/users/<%= user.id %>" class="btn btn-info" role="button">View User</a>
          <a href="http://localhost:3000/users/edit/<%= user.id %>" class="btn btn-info" role="button">Edit</a>
          <button class="btn btn-danger deleteUser" data-place-id="<%= user.id %>" role="button">Delete</button>
        </li>
        <% }); %>
    </ul>
    <a href="http://localhost:3000/users/new" class="btn btn-info" role="button">New User</a>
    <script src="/javascripts/jquery.min.js"></script>
    <script src="/javascripts/places.js"></script>
    <script src="/javascripts/bootstrap.min.js"></script>
  </body>