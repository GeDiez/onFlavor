<!DOCTYPE html>
<html>
  <head>
    <%- include("./partials/head", {title:"Dishes"}) %>
  </head>
  <body>
    <form action="http://localhost:3000/api/dishes" method="post" enctype="application/json" >
      <fieldset>
        <label for="placeid">Place_id:</label>
        <input type="text" id="placeid" name="placeid"/>
        <br />
        <label for="name">Name:</label>
        <input type="text" id="name" name="name"/>
        <br />
        <label for="price">Price:</label>
        <input type="price" id="price" name="price" />
        <br />
        <input type="submit" class="btn btn-info" value="New Place" />
      </fieldset>
    </form>
  </body>
</html>