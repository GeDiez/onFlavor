<!DOCTYPE html>
<html>
  <head>
    <%- include("./partials/head", {title:"your_title"}) %>
  </head>
  <body>
    <form action="http://localhost:3000/places/edit/<%= place.id %>" method="post" enctype="application/json" name="place">
      <fieldset>
        <label for="name">Name:</label>
        <input type="text" id="name" name="name" value= <%= place.name %> />
        <br />
        <label for="latitude">Latitude:</label>
        <input type="latitude" id="latitude" name="latitude" value=<%= place.latitude %> />
        <br />
        <label for="longitude">Longitude:</label>
        <input id="longitude" name="longitude" value=<%= place.longitude %> />
        <br />
        <input type="submit" value="Edit Place" />
      </fieldset>
    </form>
  </body>
</html>