<!DOCTYPE html>
<html>
  <head>
    <%- include("./partials/head", { title: "On flavor" }) %>
  </head>
  <body>
    <h2>Orders</h2>
    <ul>
      <% events.forEach(function(event) { %>
        <li>
          <%= event.name %>
          <a href="http://localhost:3000/orders/edit/<%= event.id %>" class="btn btn-info" role="button">Edit</a>
          <button class="btn btn-danger delete" data-place-id="<%= event.id %>" role="button">Delete</button>
        </li>
        <% }); %>
    </ul>
    <a href="http://localhost:3000/event/new" class="btn btn-info" role="button">New Order</a>
    <script src="/javascripts/jquery.min.js"></script>
    <script src="/javascripts/places.js"></script>
    <script src="/javascripts/bootstrap.min.js"></script>
  </body>
</html>