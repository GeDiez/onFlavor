<!DOCTYPE html>
<html>
  <head>
    <%- include("./partials/head", {title:"On Flavor"}) %>
  </head>
  <body>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-12">
          <h2>Edit order</h2>
        </div>
      </div>
      <div class="row">
        <form action="http://localhost:3000/orders/edit/<%= order.id %>" method="post" enctype="application/json" name="order" class="col-sm-4">
          <div class="form-group">
            <label for="eventid">Event_id:</label>
            <input type="text" id="eventid" name="eventid" value="<%= order.event_id %>" class="form-control" />
          </div>
          <div class="form-group">
            <label for="dishid">Dish_id:</label>
            <input type="dishid" id="dishid" name="dishid" value="<%= order.dish_id %>" class="form-control" />
          </div>
          <div class="form-group">
            <label for="userid">User_id:</label>
            <input id="userid" name="userid" value="<%= order.user_id %>" class="form-control" />
          </div>
          <div class="form-group">
            <label for="quantity">Quantity:</label>
            <input id="quantity" name="quantity" value="<%= order.quantity %>" class="form-control" />
          </div>
          <input type="submit" value="Save" class="btn btn-primary" />
        </form>
      </div>
    </div>
  </body>
</html>