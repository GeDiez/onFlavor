<!DOCTYPE html>
<html>
  <head>
    <%- include("./partials/head",{title:"Groups"}) %>
  </head>
  <body>
    <h2>Groups</h2>
    <ul>
      <% groups.forEach(function(groups) { %>
        <li>
          <%= groups.name %>
          <a href="http://localhost:3000/groups/<%= groups.id %>" class="btn btn-info" role="button">View Group</a>
          <a href="http://localhost:3000/groups/edit/<%= groups.id %>" class="btn btn-info" role="button">Edit</a>
          <button class="btn btn-danger deleteGroup" data-place-id="<%= groups.id %>" role="button">Delete</button>
        </li>
        <% }); %>
    </ul>
    <a href="http://localhost:3000/groups/new" class="btn btn-info" role="button">New Group</a>
    <script src="/javascripts/jquery.min.js"></script>
    <script src="/javascripts/places.js"></script>
    <script src="/javascripts/bootstrap.min.js"></script>
  </body>
</html>